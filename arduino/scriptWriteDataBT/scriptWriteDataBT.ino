#include <Wire.h>

////////////////////////////////////////////////
//                Declarations                //
////////////////////////////////////////////////
int nb = 0;

////////////////////////////////////////////////
//                 Base Code                  //
////////////////////////////////////////////////
void setup(){
	Serial.begin(9600);
	pinMode(13, OUTPUT);
}

void loop(){
	Serial.print("Coucou + ");
	nb++;
	Serial.println(nb);
	delay(5000);
}
